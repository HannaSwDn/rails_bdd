<h1>Edit article:</h1>

<%= form_for @article do |article| %>
    <p>
        <%= article.label :title %><br>
        <%= article.text_field :title %>
    </p>

    <p>
        <%= article.label :content %><br>
        <%= article.text_area :content %>
    </p>

    <p>
        <%= article.submit %>
    </p>
<% end %>